# Application Environment
APP_ENV=development
APP_DEBUG=true
APP_LOG_LEVEL=debug

# Base URL Configuration
# Development: Use http://localhost[:port]
# Production: Use your full domain (e.g., https://comic.amertech.online)
# Must be a valid URL with protocol (http:// or https://)
APP_BASE_URL=http://localhost

# Replicate API Configuration
REPLICATE_API_TOKEN=your_api_token_here
REPLICATE_WEBHOOK_SECRET=your_webhook_secret_here

# Model Configurations
# Base SDXL model (used for backgrounds and other image generation)
SDXL_MODEL_VERSION=a00d0b7dcbb9c3fbb34ba87d2d5b46c56969c84a628bf778a7fdaec30b1b99c5

# Specialized models
CARTOONIFY_MODEL=f109015d60170dfb20460f17da8cb863155823c85ece1115e1e9e4ec7ef51d3b
NLP_MODEL=5a6809ca6288247d06daf6365557e5e429063f32a21146b2a807c682652136b8

# Performance Tuning
MAX_CONCURRENT_JOBS=3
MAX_RETRIES=3
RETRY_DELAY=1
JOB_TIMEOUT=300
HEARTBEAT_INTERVAL=30
